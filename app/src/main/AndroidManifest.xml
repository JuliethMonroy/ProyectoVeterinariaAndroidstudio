<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.veterinariahealthhappy">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VeterinariaHealthHappy"
        tools:targetApi="31">
        <activity
            android:name=".EmergenciasActivity"
            android:exported="false" />
        <activity
            android:name=".UbicarveterinariasActivity"
            android:exported="false" />
        <activity
            android:name=".medicosActivity"
            android:exported="false" />
        <activity
            android:name=".serviciosActivity"
            android:exported="false" />
        <activity
            android:name=".mascotasActivity"
            android:exported="false" />
        <activity
            android:name=".veterinarioActivity"
            android:exported="false" />
        <activity
            android:name=".perfilActivity"
            android:exported="false" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />

        <activity
            android:name=".AgendarActivity"
            android:exported="false" />
        <activity
            android:name=".RegistroVeterinariasActivity"
            android:exported="false" />
        <activity
            android:name=".InicioActivity"
            android:exported="false" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />

        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".RegistroActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>