<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tvveterinario"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".veterinarioActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="226dp"
        android:layout_height="128dp"
        android:layout_marginBottom="35dp"
        app:layout_constraintBottom_toTopOf="@+id/textView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:srcCompat="@drawable/macotas" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="338dp"
        android:layout_height="62dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="9dp"
        android:text="Registro  Veterinarios"
        android:textAlignment="center"
        android:textColor="#2F5777"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="353dp"
        android:layout_height="74dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="15dp"
        android:text="Con nosotros tu mascota siempre estara completamente feliz y sano, nos apasiona atender las mascotas y brindarles la mejor atencion y que disfruten el mejor servicio!"
        android:textAlignment="center"
        android:textColor="#0C0C0C"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/btnregistrarveterinario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/btnregistrarveterinario"
        android:layout_width="193dp"
        android:layout_height="48dp"
        android:layout_marginBottom="44dp"
        android:backgroundTint="#234E4A"
        android:foregroundTint="#009688"
        android:text="Registrar"
        app:icon="@drawable/huella__1_"
        app:iconTint="#070707"
        app:layout_constraintBottom_toTopOf="@+id/textView27"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />


    <Button
        android:id="@+id/btnservicios"
        android:layout_width="81dp"
        android:layout_height="65dp"
        android:layout_marginBottom="88dp"
        android:backgroundTint="#F7EFF1F1"
        app:icon="@drawable/icons8_volver_64"
        app:iconTint="#009688"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27"
        app:rippleColor="#F7F4F3F3" />

    <androidx.cardview.widget.CardView
        android:id="@+id/textView27"
        android:layout_width="356dp"
        android:layout_height="136dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/btnservicios"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnregistrarveterinario">

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/check" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="47dp"
            android:layout_height="144dp"
            app:srcCompat="@drawable/check" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="48dp"
            android:layout_height="239dp"
            app:srcCompat="@drawable/check" />

        <ImageView
            android:id="@+id/imageView28"
            android:layout_width="220dp"
            android:layout_height="150dp"
            android:layout_marginStart="80dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="300dp"
            android:textAlignment="center"
            app:srcCompat="@drawable/opciones" />

    </androidx.cardview.widget.CardView>

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="btnregistrarveterinario"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>